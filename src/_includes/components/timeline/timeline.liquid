<div class="container">
  <div class="page-header center">
    <h1 id="timeline">{{timelineTitle}}</h1>
  </div>
  <ul class="timeline">
  {% for cv in cvList %}
    <li class="{% cycle '' , 'timeline-inverted' %}">
      <div class="timeline-badge">
        {% if cv.current %}<i class="bi-check"></i>{% else %}<i class="bi-archive"></i>{% endif %}
      </div>
      <div class="timeline-panel">
        <div class="timeline-heading">
          <h3 class="timeline-title">{{cv.client}}</h3>
          <hr />
          <h4 class="timeline-title">{{cv.title}}</h4>
          <p>
            <small class="text-muted">
              <i class="bi-calendar2-week"></i> {{ cv.date }}
            </small>
          </p>
        </div>
        <div class="timeline-body">
        {% for line in cv.desc %}
          <p>{{line}}</p>
        {% endfor %}
      </div>
    </div>
  </li>
  {% endfor %}
  </ul>
</div>
